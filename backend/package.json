{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "18.x.x",
    "npm": "9.x.x"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:type": "npx tsc --noEmit",
    "lint:js": "npx eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:prettify": "prettier --check \"**/*.{ts,tsx,json,md,scss,html,js}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json",
    "test:e2e:cov": "jest --config ./jest-e2e.json --coverage",
    "seed": "ts-node prisma/seeds/seed.ts",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./tests/coverage/packages",
    "testEnvironment": "node"
  },
  "dependencies": {
    "@fastify/static": "6.10.2",
    "@nestjs/common": "10.0.3",
    "@nestjs/core": "10.0.3",
    "@nestjs/platform-express": "10.0.3",
    "@nestjs/platform-fastify": "10.0.3",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/swagger": "7.0.10",
    "@prisma/client": "5.0.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@nestjs/testing": "10.1.0",
    "eslint-plugin-jest": "27.2.3",
    "prisma": "5.0.0"
  }
}
