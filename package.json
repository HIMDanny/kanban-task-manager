{
  "name": "kanban-task-manager",
  "version": "1.0.0",
  "description": "App for task management",
  "scripts": {
    "prettify": "prettier --write \"**/*.{ts,tsx,json,md,scss,html,js,cjs}\"",
    "prepare": "husky install",
    "lint:css": "npm run lint:css --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "lint:prettify": "prettier --check \"**/*.{ts,tsx,json,md,scss,html,js}\"",
    "lint:all": "npm run lint:prettify && npm run lint:css && npm run lint"
  },
  "private": true,
  "engines": {
    "node": "18.x.x",
    "npm": "9.x.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HIMDanny/kanban-task-manager.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HIMDanny/kanban-task-manager/issues"
  },
  "homepage": "https://github.com/HIMDanny/kanban-task-manager#readme",
  "workspaces": [
    "backend",
    "client"
  ],
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@ls-lint/ls-lint": "2.0.0",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "35.0.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-function-name": "2.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonar": "0.12.0",
    "eslint-plugin-unicorn": "47.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "eslint-plugin-typescript-sort-keys": "2.3.0"
  }
}
